      $ every 5 (const $ iter 2 $ faststart $ chop 8 $ n ((run 3) + "<3 4>" * "3") # s "rumble" # speed "-2.5" # gain "0.8" # end "0.5")
      $ every 3 (superimpose ((slow 2) . (|*| speed "0.5"))) 
