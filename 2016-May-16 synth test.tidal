-- synth test 2016 May 18
-- pixelpusher (AKA EVAN RASKOB)

bps (130/120)

d4 $ every 6 (const $ n "-12*2 -9*2 -7*2 0" # s "ffmoog" # gain "1.5" # speed "0.75")
   $ every 8 (# gain "0") $ every 4 (# up "-12") $ n "0*2 -9*2 -7*2 2" # s "ffmoog" # gain "1.4" # speed (slow 8 $ scale 0.2 1 sine1)

d2 $ stack [
  sound "[hh:5 hh]*4",
  every 4 ((density 2) . (degradeBy 0.25)) $ sound "[~ cp:3]*4",
  sound "~ ~ [sn:1 sn:2?] ~",
  whenmod 4 2 (ghost) $ sound "cy808*8"
]

d2 $ n "0 2*2 2 4" # s "bd808" # up "0 12*2 7*2 0"

d3 $ s "ho([4 4 4 [12 5]]/4,8,1)" # gain "0.8"

d5 $ ghost $ s "ffmoog ~ pmsin*2 ~" # n "c2 c4" # gain "1.4 1" # speed (slow 8 $ scale 0.2 1 sine1)

solo $ d4 $ n "-24*2 -12*4 36*2 -12" # s "ffmoog" # gain "1.4" # speed (slow 8 $ scale 0.2 1 sine1)

d2 $ every 16 (const $ s "sn606([16 12 24 32],8)" # n "3" # gain "0.8" # speed "0.75") $ every 8 (slow 2) $ ghost $ stack [
  s "ho(2,8,1)" # n "1",
  every 4 (density 2) $ s "sn([4 8 4 4]/2,8)" # speed "0.25" # gain "1"
]

solo $ d4 $ n "48 24*2 48*2 24*2" # s "ffmoog" # gain "1.3" # speed (slow 4 $ scale 0.2 1 saw1)

d3 $ ghost $ every 4 ((density 2) . degradeBy 0.25) $ whenmod 4 2 (density 2) $ sound "cy808:5*8"

d3 $ slow 1 $ ghost $ every 4 ((density 2) . degradeBy 0.25) $ sound "cy808:8([8 16],8)" # gain "0.85"

d5 $ sound "bd([4 12 8 4]/2,8)" # up "-3"

solo $ d5 $ stack [
  sound "bd([4 5],8)",
  degradeBy 0.5 $ ghost $ every 2 (const $ s "sn sn" ) $ s "sn*4",
  degradeBy 0.25 $ ghost $ sound "cy808:6(8,8,1)"
]

d5 $ silence

d4 $ degradeBy (0.25) $ density 2 $ n "c3?*2 f4? g3?*2 a5" # s "pmsin" # up "-12" # speed slow 8 (scale 0.2 1 sine1) # gain "1.2"
